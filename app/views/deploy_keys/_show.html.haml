%tr
  %td
    %a{:href => project_deploy_key_path(key.project, key)}
      %strong= key.title
  %td
    %span.update-author
      = t('.added')
      = time_ago_in_words(key.created_at)
      = t('.ago')
  %td
    = link_to t('.remove'), project_deploy_key_path(key.project, key), :confirm => 'Are you sure?', :method => :delete, :class => "danger btn delete-key small right"

