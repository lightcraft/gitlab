= render "repositories/head"
- if can? current_user, :admin_project, @project
  .alert-message.block-message
    = t('alert_message')
    = link_to new_project_deploy_key_path(@project), :class => "btn small", :title => t('.title_new_key') do
      = t('.add_deploy_key')

- if @keys.any?
  %table
    - @keys.each do |key|
      = render(:partial => 'show', :locals => {:key => key})
